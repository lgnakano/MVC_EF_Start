            <td>@Model.company2.iexId</td>
            <td>@Model.company2.Quotes.Count</td>
        </tr>
    </table>
    <p>Quote</p>
    <table>
        <tr>
            <th>Quote Id</th>
            <th>Date</th>
            <th>Open</th>
            <th>High</th>
            <th>Low</th>
            <th>Close</th>
            <th>Volume</th>
            <th>Unadj. Vol</th>
            <th>Change</th>
            <th>Change%</th>
            <th>VWAP</th>
            <th>Label</th>
            <th>Change over Time</th>
            <th>Symbol</th>
        </tr>
        @foreach (var item in Model.company2.Quotes)
        {
            <tr>
                <td>@item.QuoteId</td>
                <td>@item.date</td>
                <td>@item.open</td>
                <td>@item.high</td>
                <td>@item.low</td>
                <td>@item.close</td>
                <td>@item.volume</td>
                <td>@item.unadjustedVolume</td>
                <td>@item.change</td>
                <td>@item.changePercent</td>
                <td>@item.vwap</td>
                <td>@item.label</td>
                <td>@item.changeOverTime</td>
                <td>@item.symbol</td>
            </tr>
        }
    </table>
    <p>Quote</p>
    <table>
        <tr>
            <th>Quote Id</th>
            <th>Date</th>
            <th>Open</th>
            <th>High</th>
            <th>Low</th>
            <th>Close</th>
            <th>Volume</th>
            <th>Unadj. Vol</th>
            <th>Change</th>
            <th>Change%</th>
            <th>VWAP</th>
            <th>Label</th>
            <th>Change over Time</th>
            <th>Symbol</th>
        </tr>
        <tr>
            <td>@Model.quote1.QuoteId</td>
            <td>@Model.quote1.date</td>
            <td>@Model.quote1.open</td>
            <td>@Model.quote1.high</td>
            <td>@Model.quote1.low</td>
